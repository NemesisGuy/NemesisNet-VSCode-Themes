    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#88a9ff99",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#ff0080"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#ff66b3"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#ffff66"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#ff66b3"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#e6f7ff"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#00ffff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#ff0050"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage"
            ],
            "settings": {
                "foreground": "#00ffff"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#ffff00",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Inherited class",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#6bf4ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#6bf4ff"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#ff66b3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tag name",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#00ffff"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ff0080"
            }
        },
        {
            "name": "Library function",
            "scope": [
                "support.function",
                "support.macro"
            ],
            "settings": {
                "foreground": "#6bf4ff"
            }
        },
        {
            "name": "Library constant",
            "scope": [
                "support.constant"
            ],
            "settings": {
                "foreground": "#ffff66"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#00ffaa"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#ff0050",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#00ffff"
            }
        },
        {
            "name": "Markdown heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#00ffff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#ffff00",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#ff0080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#6bf4ff",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#00ffaa"
            }
        }
    ]
}