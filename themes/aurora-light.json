    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#0c102466",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#00a882"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#c91b65"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#ff9500"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#c91b65"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#0c1024"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#6a3afb",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#00c6ff"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage"
            ],
            "settings": {
                "foreground": "#6a3afb"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#ff9500",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Inherited class",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#00a882",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#4a27a8"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#c91b65",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tag name",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#6a3afb"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#00a882"
            }
        },
        {
            "name": "Library function",
            "scope": [
                "support.function",
                "support.macro"
            ],
            "settings": {
                "foreground": "#4a27a8"
            }
        },
        {
            "name": "Library constant",
            "scope": [
                "support.constant"
            ],
            "settings": {
                "foreground": "#ff9500"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#00a882"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#c91b65",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#6a3afb"
            }
        },
        {
            "name": "Markdown heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#6a3afb",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#ff9500",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#00a882",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#00c6ff",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#00a882"
            }
        }
    ]
}