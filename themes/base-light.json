    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#0a0e2766",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#008B6A"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#E65100"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FF9800"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#E65100"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#0a0e27"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#1E88E5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#1E88E5"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage"
            ],
            "settings": {
                "foreground": "#1E88E5"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#FF9800",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Inherited class",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#008B6A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#0f4c81"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#E65100",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tag name",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#1E88E5"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#008B6A"
            }
        },
        {
            "name": "Library function",
            "scope": [
                "support.function",
                "support.macro"
            ],
            "settings": {
                "foreground": "#0f4c81"
            }
        },
        {
            "name": "Library constant",
            "scope": [
                "support.constant"
            ],
            "settings": {
                "foreground": "#FF9800"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#008B6A"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#E65100",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#1E88E5"
            }
        },
        {
            "name": "Markdown heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#1E88E5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#FF9800",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#008B6A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#1E88E5",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#008B6A"
            }
        }
    ]
}