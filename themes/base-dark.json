    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#F4F4F466",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#00C2A8"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FF8A50"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FFB347"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#FF8A50"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#F4F4F4"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#2979FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#2979FF"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage"
            ],
            "settings": {
                "foreground": "#2979FF"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#FFB347",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Inherited class",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#00C2A8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#2979FF"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#FF8A50",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tag name",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#2979FF"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#00C2A8"
            }
        },
        {
            "name": "Library function",
            "scope": [
                "support.function",
                "support.macro"
            ],
            "settings": {
                "foreground": "#2979FF"
            }
        },
        {
            "name": "Library constant",
            "scope": [
                "support.constant"
            ],
            "settings": {
                "foreground": "#FFB347"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#00C2A8"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#FF6F3C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#2979FF"
            }
        },
        {
            "name": "Markdown heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#2979FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#FFB347",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#00C2A8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#2979FF",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#00C2A8"
            }
        }
    ]
}